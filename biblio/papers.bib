	Keywords = {Trans-Activators, Animals, Muscle: Skeletal, Oncogene Proteins, 3T3 Cells, Transcription Factors, Signal Transduction, DNA-Binding Proteins, Humans, Mice, Hedgehog Proteins, Muscle Development, Cell Lineage, Myogenic Regulatory Factor 5, Muscle Proteins, Stem Cells, Gene Expression Regulation: Developmental, Mice: Transgenic},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Emerson/2002/Genes%20&%20development/Gustafsson_Myf5%20is%20a%20direct%20target.pdf},
