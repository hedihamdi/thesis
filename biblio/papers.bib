	Doi = {10.1371/journal.pcbi.0030189},
	Journal = {PLoS Comput Biol},
	Keywords = {Models: Statistical, Models: Biological, Nonlinear Dynamics, Metabolic Networks and Pathways, Probability, Monte Carlo Method, Sensitivity and Specificity, Systems Biology, Half-Life, Algorithms, Computer Simulation, Meta-Analysis as Topic},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Sethna/2007/PLoS%20Comput%20Biol/Gutenkunst_Universally%20sloppy%20parameter%20sensitivities%20in.pdf},
	Month = {Oct},
	Number = {10},
	Pages = {1871--78},
	Pii = {07-PLCB-RA-0044},
	Pmid = {17922568},
