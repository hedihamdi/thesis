	Keywords = {Molecular Sequence Data, Binding Sites, Base Sequence, DNA Transposable Elements, Gene Expression Regulation: Developmental, Promoter Regions: Genetic, Amino Acid Sequence, Transcription Factors, Amino Acid Motifs, Humans, Mice, Alu Elements, Animals, Protein Binding},
	Language = {eng},
