	Keywords = {Myoblasts, Mice, Hindlimb, Endothelial Cells, Signal Transduction, Wnt Proteins, PAX7 Transcription Factor, beta Catenin, Embryo: Mammalian, Animals, Fetus, Paired Box Transcription Factors, Muscle Development},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Kardon/2009/Genes%20&%20development/Hutcheson_Embryonic%20and%20fetal%20limb%20myogenic.pdf},
