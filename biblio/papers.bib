	Mesh = {Animals; Base Sequence; Binding Sites; DNA; DNA-Binding Proteins; Homeodomain Proteins; Mice; Models, Molecular; Nucleic Acid Conformation; Nylons; Protein Binding; Protein Folding; Protein Structure, Secondary; Transcription Factors},
	Month = {Mar},
