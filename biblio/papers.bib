	Doi = {10.1371/journal.pcbi.0020164},
	Journal = {PLoS Comput Biol},
	Keywords = {Models: Genetic, Binding Sites, Algorithms, Sequence Analysis: DNA, Gene Expression Regulation, Protein Binding, Transcription Factors, Computer Simulation, Transcription: Genetic, Regulatory Elements: Transcriptional},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/ten%20Wolde/2006/PLoS%20Comput%20Biol/Hermsen_Transcriptional%20regulation%20by%20competing%20transcription.pdf},
