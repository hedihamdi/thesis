keywords = {Genetic Variation, Gene Deletion, DNA Transposable Elements, Molecular Sequence Data, Humans, Sequence Analysis: DNA, Polymorphism: Single Nucleotide, Computational Biology, Genome: Human, Alu Elements}, 
date-added = {2010-11-02 14:29:41 +0100},
