	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Lewis/2003/Curr%20Biol/Lewis_Autoinhibition%20with%20transcriptional%20delay%20a.pdf},
	Month = {Aug},
