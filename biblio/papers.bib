	Journal = {Nature},
	Keywords = {Mesoderm, Gastrulation, Gene Expression Regulation: Developmental, Gene Regulatory Networks, Strongylocentrotus purpuratus, Endoderm, Body Patterning, Animals},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Davidson/2011/Nature/Peter_A%20gene%20regulatory%20network%20controlling.pdf},
	Month = {Jun},
