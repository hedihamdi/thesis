	Doi = {10.1101/gr.6249707},
	Journal = {Genome Res},
	Keywords = {Synteny, Drosophila Proteins, Animals, Drosophila melanogaster, Sequence Homology: Nucleic Acid, Genetic Variation, Genome: Insect, Evolution: Molecular, Conserved Sequence},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Ponting/2007/Genome%20Res/Heger_Evolutionary%20rate%20analyses%20of%20orthologs.pdf},
	Month = {Dec},
