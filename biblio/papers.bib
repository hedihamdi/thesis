	Doi = {10.1101/gr.4318206},
	Journal = {Genome Res},
	Keywords = {Evolution: Molecular, Introns, Models: Genetic, Proteins, Sequence Alignment, Mutation, Exons, Species Specificity, Algorithms, Animals, DNA: Intergenic, Bias (Epidemiology), Humans, Conserved Sequence, Gene Expression Regulation, Mice},
	Language = {eng},
