	Date-Modified = {2010-10-13 09:34:55 +0200},
	Doi = {10.1016/j.ydbio.2006.08.059},
	Journal = {Developmental Biology},
	Keywords = {Homeodomain Proteins, Paired Box Transcription Factors, MyoD Protein, Promoter Regions: Genetic, Mice, Somites, Extremities, Muscle: Skeletal, Intracellular Signaling Peptides and Proteins, Body Patterning, Animals, Myogenin, Muscle Development, Protein Tyrosine Phosphatases, Myogenic Regulatory Factors, Nuclear Proteins, Cell Movement, Enhancer Elements: Genetic, Mice: Knockout},
	Language = {eng},
