	Journal = {Proc Natl Acad Sci USA},
	Keywords = {Genome: Fungal, Saccharomyces cerevisiae Proteins, Humans, Biometry, Saccharomyces cerevisiae, Gene Expression Profiling, Cell Line: Tumor, Models: Statistical, Alzheimer Disease, Macromolecular Substances, Genome: Human, Oligonucleotide Array Sequence Analysis},
	Language = {eng},
