	Journal = {BMC Bioinformatics},
	Keywords = {Evolution: Molecular, Caenorhabditis elegans, Sequence Homology: Nucleic Acid, Benchmarking, Drosophila, Computer Simulation, Humans, Mice, Drosophila melanogaster, Caenorhabditis, Sensitivity and Specificity, Genomics, DNA: Intergenic, Animals, DNA: Helminth, Sequence Alignment},
	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Eisen/2004/BMC%20Bioinformatics/Pollard_Benchmarking%20tools%20for%20the%20alignment.pdf},
