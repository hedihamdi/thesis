	Language = {eng},
	Local-Url = {file://localhost/Users/Marc/Documents/Papers/Viale/2010/Proc%20Natl%20Acad%20Sci%20USA/Cavagna_Scale-free%20correlations%20in%20starling%20flocks.pdf},
